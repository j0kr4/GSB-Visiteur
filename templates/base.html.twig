<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}GSB{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('css/vendor/bootstrap/bootstrap.css') }}" rel="stylesheet">
            <link href="{{ asset('css/vendor/bootstrap/bootstrap.min.css') }}" rel="stylesheet">
            <link href="{{ asset('css/vendor/bootstrap/bootstrap-grid.css') }}" rel="stylesheet">
            <link href="{{ asset('css/vendor/bootstrap/bootstrap-reboot.css') }}" rel="stylesheet">
            <link href="{{ asset('css/gsb.css') }}" rel="stylesheet">
            <link href="{{ asset('css/nav.css') }}" rel="stylesheet">
        {% endblock %}
        {% block icon %}<link rel="icon" type="image/x-icon" href="{{ asset('img/image.jpeg') }}" />{% endblock %}
    </head>
    <body>
        {% block menu %}{% endblock %}
        {% block nav %}{% endblock %}
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
            <script src="https://unpkg.com/popper.js"></script>
            <script src="{{ asset('js/vendor/bootstrap/bootstrap.js') }}"></script>
            <script src="{{ asset('js/vendor/bootstrap/bootstrap.min.js') }}"></script>
            <script src="{{ asset('js/vendor/intl-tel-input/utils.js') }}"></script>
        {% endblock %}
    </body>
</html>
